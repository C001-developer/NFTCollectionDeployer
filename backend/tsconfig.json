{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "outDir": "dist",
      "baseUrl": ".",
      "types": ["hardhat", "node"],
      "paths": {
        "*": ["node_modules/*", "src/types/*"]
      }
    },
    "include": ["./server", "./scripts"],
    "files": ["./hardhat.config.ts"],
    "exclude": ["node_modules"]
  }
  